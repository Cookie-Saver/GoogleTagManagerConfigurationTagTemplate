___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
const createQueue = require('createQueue');
const makeInteger = require('makeInteger');
const log = require('logToConsole');

// The main Cookie Saver utility function. Methods are queued until the Cookie Saver SDK has loaded.
const getAcq = () => {
  // Return the existing 'acq' global object if available
  let acq = copyFromWindow('acq');
  if (acq) {
    return acq;
  }
  
  // Initialize the 'acq' global object to either use acq.callMethod or acq.queue
  setInWindow('acq', function() {    
    const callMethod = copyFromWindow('acq.callMethod.apply');
    if (callMethod) {           
      callInWindow('acq.callMethod.apply', null, arguments); 
    } else {       
      callInWindow('acq.queue.push', arguments);
    }
  });
  
  // Create the acq.queue
  createQueue('acq.queue');
    
  // Return the created global 'acq' object
  return copyFromWindow('acq');
};

// Get reference to the global method
const acq = getAcq();

// Call 'init' method on the 'acq' objct
// Object data is provided by input fields
acq('init', {
	endpoint: data.endpoint
});

// Call 'addCookie' on the 'acq' object to add each cookie from the UI
const cookies = data.cookies;
if (cookies) {
  cookies.forEach(function(cookie) {
	
    // Create the cookie object to be added
    var cookieConfig = {
      name: cookie.name,
      expirationDays: makeInteger(cookie.expirationDays),
      secure: cookie.secure,
      backup: cookie.backup,
      autoExtend: cookie.autoExtend,
      safariOnly: cookie.safariOnly
    };
    
    // Only add the domain property of the value is defined by the user
    if (cookie.domain) {
      cookieConfig.domain = cookie.domain;
    }
    
    // Add the cookie to the 'acq' object
	acq('addCookie', cookieConfig);
  });
}

// Call 'start' method on the 'acq' object to start Cookie Saver
acq('start');

const url = 'https://cdn.accutics.net/cookiesaver/3/main.min.js';
injectScript(url, data.gtmOnSuccess, data.gtmOnFailure, url);


